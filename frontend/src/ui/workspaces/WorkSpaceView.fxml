<?import javafx.scene.control.SplitPane?>

<!-- Split Pane containing the workspace view and edit bar !-->
<?import ui.workspaces.listspace.ListSpaceView?>
<?import ui.workspaces.editbar.EditBarContainer?>
<?import ui.workspaces.categorybar.CategoryListView?>

<fx:root type="javafx.scene.control.SplitPane"
         stylesheets="@WorkSpaceView.css"
         xmlns="http://javafx.com/javafx/15.0.1"
         xmlns:fx="http://javafx.com/fxml/1"
         fx:id="viewContainer">

    <CategoryListView fx:id="categoryListView" />

    <!-- Runtime: ListView -->
    <ListSpaceView fx:id="listSpaceView" />

    <!-- Task EditBar -->
    <EditBarContainer fx:id="editBarContainer"/>

</fx:root>
